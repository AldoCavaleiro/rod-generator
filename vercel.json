{
  "version": 2,
  "functions": {
    "api/generate.js": {
      "runtime": "nodejs20.x",
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "builds": [
    { "src": "api/generate.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/generate", "dest": "/api/generate.js" }
  ]
}
